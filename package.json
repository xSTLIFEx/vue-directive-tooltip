{
  "name": "vue-directive-tooltip",
  "version": "1.5.0",
  "description": "Vue.js tooltip directive ",
  "main": "cjs/index.js",
  "browser": "dist/vueDirectiveTooltip.js",
  "module": "es/index.js",
  "jsxnext:main": "es/index.js",
  "style": "css/index.css",
  "files": [
    "dist",
    "cjs",
    "es",
    "src",
    "css"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "prepublish": "npm run build",
    "release": "release-it",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint src/*.js",
    "prebuild:sass": "node-sass src/css --output-style=compressed -o ./css",
    "build:sass": "postcss --use autoprefixer -b 'last 2 versions' < ./css/index.css | postcss --use cssnano > ./css/index.min.css",
    "build:sass:watch": "node-sass -w src/css -o ./css --source-map=true --source-map-contents --error-bell",
    "build": "npm run build:cjs && npm run build:es && npm run build:umd && npm run build:sass && npm run copy:assets",
    "build:watch": "clear && rimraf cjs && cross-env BABEL_ENV=cjs babel -w src --out-dir cjs  & npm run build:sass:watch",
    "build:es": "rimraf es && cross-env BABEL_ENV=es babel src --out-dir es",
    "build:cjs": "rimraf cjs && cross-env BABEL_ENV=cjs babel src --out-dir cjs",
    "build:umd": "rimraf dist && cross-env BABEL_ENV=es rollup -c & cross-env BABEL_ENV=es NODE_ENV=production rollup -c",
    "copy:assets": "cp ./css/*.* ./docs/css/ && rm ./docs/js/vueDirectiveTooltip.js && cp ./dist/vueDirectiveTooltip.js ./docs/js/",
    "start": "npm run build",
    "serve:doc": "cd ./docs && jekyll serve --incremental"
  },
  "keywords": [],
  "author": "Laurent Blanes (laurent.blanes@gmail.com)",
  "license": "MIT",
  "repository": "hekigan/vue-directive-tooltip",
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.17.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.22.0",
    "cross-env": "^3.1.3",
    "cssnano": "^3.10.0",
    "eslint": "^3.12.0",
    "eslint-config-standard-extended": "^1.0.1",
    "eslint-plugin-babel": "^4.0.0",
    "husky": "^0.12.0",
    "node-sass": "^4.5.0",
    "postcss": "^5.2.12",
    "postcss-cli": "^2.6.0",
    "release-it": "^2.5.1",
    "rimraf": "^2.5.4",
    "rollup": "0.39.2",
    "rollup-plugin-babel": "2.6.1",
    "rollup-plugin-buble": "0.15.0",
    "rollup-plugin-bundle-size": "1.0.1",
    "rollup-plugin-commonjs": "7.0.0",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-uglify": "1.0.1",
    "rollup-plugin-vue": "2.3.1",
    "vue": "^2.5.13"
  },
  "dependencies": {
    "popper.js": "1.12.5"
  }
}
